{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      description: '',\n      amount: 0,\n      date: '',\n      category: '',\n      categories: ['Travel', 'Food', 'Entertainment', 'Transport', 'Others']\n    };\n  },\n  methods: {\n    addExpense() {\n      const auth = localStorage.getItem('auth') || btoa('admin:admin');\n      axios.post('http://localhost:8080/api/expenses', {\n        description: this.description,\n        amount: this.amount,\n        date: this.date,\n        category: this.category\n      }, {\n        headers: {\n          'Authorization': `Basic ${auth}`\n        }\n      }).then(() => {\n        this.$emit('expense-added');\n        this.description = '';\n        this.amount = 0;\n        this.date = '';\n        this.category = '';\n      }).catch(error => console.error(error));\n    }\n  }\n};","map":{"version":3,"names":["axios","data","description","amount","date","category","categories","methods","addExpense","auth","localStorage","getItem","btoa","post","headers","then","$emit","catch","error","console"],"sources":["src/components/ExpenseForm.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Add Expense</h2>\n    <form @submit.prevent=\"addExpense\">\n      <input v-model=\"description\" placeholder=\"Description\" required />\n      <input v-model=\"amount\" type=\"number\" placeholder=\"Amount\" required />\n      <input v-model=\"date\" type=\"date\" required />\n      <select v-model=\"category\" required>\n        <option disabled value=\"\">Select Category</option>\n        <option v-for=\"cat in categories\" :key=\"cat\" :value=\"cat\">{{ cat }}</option>\n      </select>\n      <button type=\"submit\">Add</button>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      description: '',\n      amount: 0,\n      date: '',\n      category: '',\n      categories: ['Travel', 'Food', 'Entertainment', 'Transport', 'Others']\n    };\n  },\n  methods: {\n    addExpense() {\n      const auth = localStorage.getItem('auth') || btoa('admin:admin');\n      axios.post('http://localhost:8080/api/expenses', {\n        description: this.description,\n        amount: this.amount,\n        date: this.date,\n        category: this.category\n      }, {\n        headers: { 'Authorization': `Basic ${auth}` }\n      })\n      .then(() => {\n        this.$emit('expense-added');\n        this.description = '';\n        this.amount = 0;\n        this.date = '';\n        this.category = '';\n      })\n      .catch(error => console.error(error));\n    }\n  }\n};\n</script>\n"],"mappings":"AAiBA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,MAAA;MACAC,IAAA;MACAC,QAAA;MACAC,UAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAA;MACA,MAAAC,IAAA,GAAAC,YAAA,CAAAC,OAAA,YAAAC,IAAA;MACAZ,KAAA,CAAAa,IAAA;QACAX,WAAA,OAAAA,WAAA;QACAC,MAAA,OAAAA,MAAA;QACAC,IAAA,OAAAA,IAAA;QACAC,QAAA,OAAAA;MACA;QACAS,OAAA;UAAA,0BAAAL,IAAA;QAAA;MACA,GACAM,IAAA;QACA,KAAAC,KAAA;QACA,KAAAd,WAAA;QACA,KAAAC,MAAA;QACA,KAAAC,IAAA;QACA,KAAAC,QAAA;MACA,GACAY,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAD,KAAA,CAAAA,KAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}