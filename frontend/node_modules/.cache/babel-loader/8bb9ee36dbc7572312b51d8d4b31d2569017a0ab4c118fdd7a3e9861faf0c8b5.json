{"ast":null,"code":"import axios from 'axios';\nexport default {\n  data() {\n    return {\n      description: '',\n      amount: 0,\n      date: '',\n      expenses: []\n    };\n  },\n  methods: {\n    formatDate(date) {\n      return new Date(date).toLocaleDateString();\n    },\n    fetchExpenses() {\n      const auth = localStorage.getItem('auth');\n      axios.get('http://localhost:8080/api/expenses?page=0&size=10', {\n        headers: {\n          'Authorization': `Basic ${auth}`\n        }\n      }).then(response => {\n        this.expenses = response.data.expenses;\n      }).catch(error => console.error(error));\n    },\n    addExpense() {\n      const auth = localStorage.getItem('auth');\n      axios.post('http://localhost:8080/api/expenses', {\n        description: this.description,\n        amount: this.amount,\n        date: this.date\n      }, {\n        headers: {\n          'Authorization': `Basic ${auth}`\n        }\n      }).then(() => {\n        this.description = '';\n        this.amount = 0;\n        this.date = '';\n        this.fetchExpenses(); // Refresh the list\n      }).catch(error => console.error(error));\n    },\n    deleteExpense(id) {\n      const auth = localStorage.getItem('auth');\n      axios.delete(`http://localhost:8080/api/expenses/${id}`, {\n        headers: {\n          'Authorization': `Basic ${auth}`\n        }\n      }).then(() => {\n        this.fetchExpenses(); // Refresh the list\n      }).catch(error => console.error(error));\n    }\n  },\n  mounted() {\n    this.fetchExpenses();\n  }\n};","map":{"version":3,"names":["axios","data","description","amount","date","expenses","methods","formatDate","Date","toLocaleDateString","fetchExpenses","auth","localStorage","getItem","get","headers","then","response","catch","error","console","addExpense","post","deleteExpense","id","delete","mounted"],"sources":["src/components/ExpenseForm.vue"],"sourcesContent":["<template>\n  <div>\n    <h2>Add Expense</h2>\n    <form @submit.prevent=\"addExpense\">\n      <input v-model=\"description\" placeholder=\"Description\" required />\n      <input v-model.number=\"amount\" type=\"number\" placeholder=\"Amount\" required />\n      <input v-model=\"date\" type=\"date\" required />\n      <button type=\"submit\">Add</button>\n    </form>\n\n    <ul>\n      <li v-for=\"expense in expenses\" :key=\"expense.id\">\n        {{ expense.description }} - ${{ expense.amount }} - {{ formatDate(expense.date) }}\n        <button @click=\"deleteExpense(expense.id)\">Delete</button>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      description: '',\n      amount: 0,\n      date: '',\n      expenses: []\n    };\n  },\n  methods: {\n    formatDate(date) {\n      return new Date(date).toLocaleDateString();\n    },\n    fetchExpenses() {\n      const auth = localStorage.getItem('auth');\n      axios.get('http://localhost:8080/api/expenses?page=0&size=10', {\n        headers: { 'Authorization': `Basic ${auth}` }\n      })\n      .then(response => {\n        this.expenses = response.data.expenses;\n      })\n      .catch(error => console.error(error));\n    },\n    addExpense() {\n      const auth = localStorage.getItem('auth');\n      axios.post('http://localhost:8080/api/expenses', {\n        description: this.description,\n        amount: this.amount,\n        date: this.date\n      }, {\n        headers: { 'Authorization': `Basic ${auth}` }\n      })\n      .then(() => {\n        this.description = '';\n        this.amount = 0;\n        this.date = '';\n        this.fetchExpenses(); // Refresh the list\n      })\n      .catch(error => console.error(error));\n    },\n    deleteExpense(id) {\n      const auth = localStorage.getItem('auth');\n      axios.delete(`http://localhost:8080/api/expenses/${id}`, {\n        headers: { 'Authorization': `Basic ${auth}` }\n      })\n      .then(() => {\n        this.fetchExpenses(); // Refresh the list\n      })\n      .catch(error => console.error(error));\n    }\n  },\n  mounted() {\n    this.fetchExpenses();\n  }\n};\n</script>\n"],"mappings":"AAoBA,OAAAA,KAAA;AAEA;EACAC,KAAA;IACA;MACAC,WAAA;MACAC,MAAA;MACAC,IAAA;MACAC,QAAA;IACA;EACA;EACAC,OAAA;IACAC,WAAAH,IAAA;MACA,WAAAI,IAAA,CAAAJ,IAAA,EAAAK,kBAAA;IACA;IACAC,cAAA;MACA,MAAAC,IAAA,GAAAC,YAAA,CAAAC,OAAA;MACAb,KAAA,CAAAc,GAAA;QACAC,OAAA;UAAA,0BAAAJ,IAAA;QAAA;MACA,GACAK,IAAA,CAAAC,QAAA;QACA,KAAAZ,QAAA,GAAAY,QAAA,CAAAhB,IAAA,CAAAI,QAAA;MACA,GACAa,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAD,KAAA,CAAAA,KAAA;IACA;IACAE,WAAA;MACA,MAAAV,IAAA,GAAAC,YAAA,CAAAC,OAAA;MACAb,KAAA,CAAAsB,IAAA;QACApB,WAAA,OAAAA,WAAA;QACAC,MAAA,OAAAA,MAAA;QACAC,IAAA,OAAAA;MACA;QACAW,OAAA;UAAA,0BAAAJ,IAAA;QAAA;MACA,GACAK,IAAA;QACA,KAAAd,WAAA;QACA,KAAAC,MAAA;QACA,KAAAC,IAAA;QACA,KAAAM,aAAA;MACA,GACAQ,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAD,KAAA,CAAAA,KAAA;IACA;IACAI,cAAAC,EAAA;MACA,MAAAb,IAAA,GAAAC,YAAA,CAAAC,OAAA;MACAb,KAAA,CAAAyB,MAAA,uCAAAD,EAAA;QACAT,OAAA;UAAA,0BAAAJ,IAAA;QAAA;MACA,GACAK,IAAA;QACA,KAAAN,aAAA;MACA,GACAQ,KAAA,CAAAC,KAAA,IAAAC,OAAA,CAAAD,KAAA,CAAAA,KAAA;IACA;EACA;EACAO,QAAA;IACA,KAAAhB,aAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}